{
  "name": "package-json-versions",
  "displayName": "Package JSON Versions",
  "description": "Uses your shell npm to see if there are new versions for packages",
  "license": "MIT",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:json"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "package-versions.clear-cache",
        "title": "Package Versions: Clear Cache"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint . --ext .ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "8.x",
    "@types/mocha": "10.x",
    "@types/node": "16.x",
    "eslint-config": "file:eslint-config",
    "jest":"28.x",
    "glob": "8.x",
    "mocha": "10.x",
    "typescript": "4.9.x",
    "ts-loader": "9.x",
    "webpack": "5.x",
    "webpack-cli": "5.x",
    "@vscode/test-electron": "2.x"
  },
  "dependencies": {
    "semver": "7.x"
  },
  "peerDependencies": {},
  "resolutions": {}
}
